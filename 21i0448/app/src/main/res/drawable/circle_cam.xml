<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    >

    <!-- Remove the solid color and add a stroke with desired color and width -->
    <stroke
        android:color="#157177"
        android:width="5dp"
        />
    <solid
        android:color="#FFFFFF"
        ></solid>
    <!-- Set the size of the circle (adjust as needed) -->
    <size
        android:width="75dp"
        android:height="75dp" />

</shape>
