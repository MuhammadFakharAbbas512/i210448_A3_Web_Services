<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    >
    <!-- Remove the solid color FFC90A and add a stroke with desired color and width -->
    <stroke
        android:color="#FFFFFF"
        android:width="2dp"
        />
    <solid android:color="#9C9C9C"></solid>
    <!-- Set the size of the circle (adjust as needed) -->
    <size
        android:width="30dp"
        android:height="30dp" />

</shape>
